<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:keepScreenOn="true"
    tools:context="com.multiview.immi.mutipleview.VulMulActivity">

    <FrameLayout
        android:id="@+id/Board1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center" >
        <ImageView
            android:id="@+id/it1"
            android:visibility="invisible"
            android:src="@drawable/thief"
            android:layout_width="40dp"
            android:layout_height="40dp" />
        <ImageView
            android:id="@+id/it2"
            android:visibility="invisible"
            android:src="@drawable/queen"
            android:layout_width="40dp"
            android:layout_height="40dp" />
        <ImageView
            android:id="@+id/it3"
            android:visibility="invisible"
            android:src="@drawable/king"
            android:layout_width="40dp"
            android:layout_height="40dp" />
        <ImageView
            android:id="@+id/it4"
            android:visibility="invisible"
            android:src="@drawable/prince"
            android:layout_width="40dp"
            android:layout_height="40dp" />
        <ImageView
            android:id="@+id/it5"
            android:visibility="invisible"
            android:src="@drawable/princess"
            android:layout_width="40dp"
            android:layout_height="40dp" />
        <ImageView
            android:id="@+id/it6"
            android:visibility="invisible"
            android:src="@drawable/warrior"
            android:layout_width="40dp"
            android:layout_height="40dp" />

    </FrameLayout>

</RelativeLayout>
